UT\b0  \b VARCHAR2\b0 );\line\line   \b PROCEDURE\b0  SP_CUENTA_REGISTRO(V_TIPO    \b IN\b0  \b VARCHAR2\b0 ,\line                                V_BUSCA   \b IN\b0  \b VARCHAR2\b0 ,\line                                V_RETORNO \b OUT\b0  \b VARCHAR2\b0 );\line   \b PROCEDURE\b0  P_SEL_PERFIL_DM(V_USUARIO \b IN\b0  \b VARCHAR2\b0 ,\line                             VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_SEL_MENU_DM(V_PERFIL \b IN\b0  \b VARCHAR2\b0 , VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_SEL_BOTON_DM(V_PERFIL \b IN\b0  \b VARCHAR2\b0 , VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_SEL_MENU_BOTON_DM(V_PERFIL  \b IN\b0  \b VARCHAR2\b0 ,\line                                 V_BOTON   \b IN\b0  \b VARCHAR2\b0 ,\line                                 VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line\b PROCEDURE\b0  P_SEL_BOTON_X_MENU(V_OPCION_MENU \b IN\b0  \b VARCHAR2\b0 ,\line                              VO_CURSOR     \b OUT\b0  REFCUR_TYPE);\line\b PROCEDURE\b0  P_SEL_LISTA_DM(V_CODIGO \b IN\b0  \b VARCHAR2\b0 , VO_CURSOR \b OUT\b0  REFCUR_TYPE);      \line\b PROCEDURE\b0  P_SEL_LISTA_DM_GDB(V_X1 \b IN\b0  \b VARCHAR2\b0 , V_X2 \b IN\b0  \b VARCHAR2\b0 ,V_Y1 \b IN\b0  \b VARCHAR2\b0 ,V_Y2 \b IN\b0  \b VARCHAR2\b0 , VO_CURSOR \b OUT\b0  REFCUR_TYPE);                       \line\b PROCEDURE\b0  P_SEL_LISTA_X_COORDENADA(V_X1 \b IN\b0  \b VARCHAR2\b0 , V_X2 \b IN\b0  \b VARCHAR2\b0 ,V_Y1 \b IN\b0  \b VARCHAR2\b0 ,V_Y2 \b IN\b0  \b VARCHAR2\b0 ,\line                              VO_CURSOR     \b OUT\b0  REFCUR_TYPE);\line\b PROCEDURE\b0  P_SEL_DATA_X_CODIGODM(V_CODIGO \b IN\b0  \b VARCHAR2\b0 ,\line                              VO_CURSOR     \b OUT\b0  REFCUR_TYPE);\line\b END\b0  PKG_SIGCATMIN;\line\b\f1 '**********************************************************************\f0\par
