{\*\generator Msftedit 5.41.15.1507;}\viewkind4\uc1\pard\cf1\b\f0\fs16 CREATE\b0  \b OR\b0  \b REPLACE\b0  \b PACKAGE\b0  PKG_SIGCATMIN \b is\b0\line   \b TYPE\b0  REFCUR_TYPE \b IS\b0  \b REF\b0  \b CURSOR\b0 ;\line\line   \b PROCEDURE\b0  P_SEL_DATOS_CARTA(V_CAMPO   \b IN\b0  \b VARCHAR2\b0 ,\line                               V_DATO    \b IN\b0  \b VARCHAR2\b0 ,\line                               VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_SEL_DM_UNIQUE(V_CODIGO  \b IN\b0  \b VARCHAR2\b0 ,\line                             V_TIPO    \b IN\b0  \b VARCHAR2\b0 ,\line                             VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_SEL_DATOS_DM(V_CODIGO \b IN\b0  \b VARCHAR2\b0 , VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_SEL_DATOS_UBIGEO(V_CODIGO  \b IN\b0  \b VARCHAR2\b0 ,\line                                VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_VERIFICA_USUARIO(V_USUARIO \b IN\b0  \b VARCHAR2\b0 ,\line                                VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_SEL_AREA_RESERVA(V_TIPO    \b IN\b0  \b VARCHAR2\b0 ,\line                                V_BUSCA   \b IN\b0  \b VARCHAR2\b0 ,\line                                VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  SP_VERIFICA_OBSERVACION(V_CG_CODIGO \b IN\b0  \b VARCHAR2\b0 ,\line                                     V_RETORNO   \b OUT\b0  \b VARCHAR2\b0 );\line\line   \b PROCEDURE\b0  SP_INS_UPD_OBSERVACION(V_CG_CODIGO \b IN\b0  \b VARCHAR2\b0 ,\line                                    V_CG_CODEVA \b IN\b0  \b VARCHAR2\b0 ,\line                                    V_ET_INDICA \b IN\b0  \b VARCHAR2\b0 ,\line                                    V_ET_USUFOR \b IN\b0  \b VARCHAR2\b0 ,\line                                    V_US_LOGUSE \b IN\b0  \b VARCHAR2\b0 ,\line                                    V_OPCION    \b IN\b0  \b VARCHAR2\b0 ,\line                                    V_RETORNO   \b OUT\b0  \b VARCHAR2\b0 );\line\line   \b PROCEDURE\b0  SP_CUENTA_REGISTRO(V_TIPO    \b IN\b0  \b VARCHAR2\b0 ,\line                                V_BUSCA   \b IN\b0  \b VARCHAR2\b0 ,\line                                V_RETORNO \b OUT\b0  \b VARCHAR2\b0 );\line   \b PROCEDURE\b0  P_SEL_PERFIL_DM(V_USUARIO \b IN\b0  \b VARCHAR2\b0 ,\line                             VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_SEL_MENU_DM(V_PERFIL \b IN\b0  \b VARCHAR2\b0 , VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_SEL_BOTON_DM(V_PERFIL \b IN\b0  \b VARCHAR2\b0 , VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line   \b PROCEDURE\b0  P_SEL_MENU_BOTON_DM(V_PERFIL  \b IN\b0  \b VARCHAR2\b0 ,\line                                 V_BOTON   \b IN\b0  \b VARCHAR2\b0 ,\line                                 VO_CURSOR \b OUT\b0  REFCUR_TYPE);\line\b PROCEDURE\b0  P_SEL_BOTON_X_MENU(V_OPCION_MENU \b IN\b0  \b VARCHAR2\b0 ,\line                              VO_CURSOR     \b OUT\b0  REFCUR_TYPE);\line\b PROCEDURE\b0  P_SEL_LISTA_DM(V_CODIGO \b IN\b0  \b VARCHAR2\b0 , VO_CURSOR \b OUT\b0  REFCUR_TYPE);                             \line\line\b END\b0  PKG_SIGCATMIN;\line\b\f1\par
